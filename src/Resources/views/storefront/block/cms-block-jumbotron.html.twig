{% block block_jumbotron %}
    {% set element = block.slots.getSlot('main') %}
    {% set columns = 1 %}
    <div class="col-12 cms-block-jumbotron" data-cms-element-id="{{ element.id }}">
        {% block block_jumbotron_content_inner %}
            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
        {% endblock %}
    </div>
{% endblock %}
